    "%matplotlib inline\n",
    "\n",
    "np.info(fresnel)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "ssa, cca = fresnel(np.linspace(-4, 4, 1000))\n",
    "\n",
    "plt.plot(ssa, cca, 'b-')\n",
    "plt.axes().set_aspect('equal')\n",
    "plt.show()"
   ]
